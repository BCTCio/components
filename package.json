{
  "displayName": "BCTC Components",
  "version": "2.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "cross-env NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify",
    "package-update": "npm i --package-lock-only && yarn"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@bctc/components",
  "author": "mxdi9i7",
  "module": "dist/bctc-components.esm.js",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@tailwindcss/forms": "^0.4.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "tailwindcss": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.10",
    "@hookstate/core": "3.0.13",
    "@mantine/hooks": "^5.2.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "compressorjs": "^1.1.1",
    "gifsicle-wasm-browser": "^1.5.16"
  }
}
